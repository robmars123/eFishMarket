{
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "TenantId": "a663a91d-6646-468f-a4d5-db1639e09bd4", //Also known - Directory ID
    "ClientId": "a807f80a-fa69-45aa-ab32-49ccc24383b9" //Also known - Application ID
  },
  "ConnectionStrings": {
    //local sql instance
    //"DefaultConnection": "Data Source=TENTON\\DEV;Initial Catalog=EFM.Database;Integrated Security=True;Encrypt=True;Trust Server Certificate=True"
    //docker sql instance container or docker connecting to local SQL instance
    //"DefaultConnection": "Data Source=host.docker.internal,1433;Initial Catalog=EFM.Database;User Id=sa;Password=Password@123;Encrypt=True;Trust Server Certificate=True"
    //azure cloud
    "DefaultConnection": "Server=tcp:efm.database.windows.net,1433;Initial Catalog=EFM.Database;Persist Security Info=False;User ID=efm-admin;Password=Password@123;MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://localhost:5341/" }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "Evently.Api"
    }
  }
}
